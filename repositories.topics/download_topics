#!/bin/bash

wget -q "https://github.com/topics/ionic5?o=desc&s=start"   -O reps_by_most_stars
wget -q "https://github.com/topics/ionic5?o=desc&s=updated" -O reps_by_recently_updates

cat reps_by*								|\
grep "go to repository" 					|\
sed '1,$s/^.*href="//'						|\
grep text-bold								|\
cut -d'"' -f1								|\
sort										|\
uniq										|\
while read REP
do 
	echo git_clone  https://github.com$REP
done




